<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:property name="joint_effort" value="56.0"/>
  <xacro:property name="joint_velocity" value="3.141592653589793"/>
  <xacro:property name="joint_damping" value="0.0"/>
  <xacro:property name="joint_friction" value="0.0"/>

  <xacro:property name="link1_mass" value="2.0"/>
  <xacro:property name="link2_mass" value="2.0"/>
  <xacro:property name="link3_mass" value="3.42"/>
  <xacro:property name="link4_mass" value="1.26"/>
  <xacro:property name="link5_mass" value="0.8"/>
  <xacro:property name="link6_mass" value="0.8"/>
  <xacro:property name="flange_mass" value="0.35"/>

  <xacro:macro name="link_inertial" params="mass ixx iyy izz">
    <inertial>
      <origin xyz="0 0 0.0" rpy="0 0 0"/>
      <mass value="${mass}"/>
      <inertia
        ixx="${ixx}" ixy="0.0" ixz="0.0"
        iyy="${iyy}" iyz="0.0"
        izz="${izz}"/>
    </inertial>
  </xacro:macro>

  <xacro:macro name="material_visual" params="ref_link ambient diffuse specular">
    <gazebo reference="${ref_link}">
      <visual>
        <material>
          <ambient>${ambient}</ambient>
          <diffuse>${diffuse}</diffuse>
          <specular>${specular}</specular>
        </material>
      </visual>
    </gazebo>
  </xacro:macro>

  <xacro:macro name="ur3e_arm" params="base_link flange_link prefix *origin">

    <link name="${prefix}link1">
      <xacro:link_inertial mass="${link1_mass}" ixx="0.0030531654454" iyy="0.0030531654454" izz="0.005625"/>
      <visual>
        <geometry>
          <mesh filename="file://$(find ur_custom_description)/meshes/ur3e/visual/base.dae"/>
        </geometry>
        <origin xyz="0 0 0" rpy="0 0 3.141592653589793"/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find ur_custom_description)/meshes/ur3e/collision/base.stl"/>
        </geometry>
        <origin xyz="0 0 0" rpy="0 0 3.141592653589793"/>
      </collision>
    </link>

    <link name="${prefix}link2">
      <xacro:link_inertial mass="${link2_mass}" ixx="0.008093163429399999" iyy="0.008093163429399999" izz="0.005625"/>
      <visual>
        <geometry>
          <mesh filename="file://$(find ur_custom_description)/meshes/ur3e/visual/shoulder.dae"/>
        </geometry>
        <origin xyz="0 0 0" rpy="0 0 3.141592653589793"/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find ur_custom_description)/meshes/ur3e/collision/shoulder.stl"/>
        </geometry>
        <origin xyz="0 0 0" rpy="0 0 3.141592653589793"/>
      </collision>
    </link>

    <link name="${prefix}link3">
      <xacro:link_inertial mass="${link3_mass}" ixx="0.021728483221103233" iyy="0.021728483221103233" izz="0.00961875"/>
      <visual>
        <geometry>
          <mesh filename="file://$(find ur_custom_description)/meshes/ur3e/visual/upperarm.dae"/>
        </geometry>
        <origin xyz="0 0 0.12" rpy="1.5707963267948966 0 -1.5707963267948966"/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find ur_custom_description)/meshes/ur3e/collision/upperarm.stl"/>
        </geometry>
        <origin xyz="0 0 0.12" rpy="1.5707963267948966 0 -1.5707963267948966"/>
      </collision>
    </link>

    <link name="${prefix}link4">
      <xacro:link_inertial mass="${link4_mass}" ixx="0.0065445675821719194" iyy="0.0065445675821719194" izz="0.00354375"/>
      <visual>
        <geometry>
          <mesh filename="file://$(find ur_custom_description)/meshes/ur3e/visual/forearm.dae"/>
        </geometry>
        <origin xyz="0.0 0 0.027" rpy="1.5707963267948966 0 -1.5707963267948966"/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find ur_custom_description)/meshes/ur3e/collision/forearm.stl"/>
        </geometry>
        <origin xyz="0.0 0 0.027" rpy="1.5707963267948966 0 -1.5707963267948966"/>
      </collision>
    </link>

    <link name="${prefix}link5">
      <xacro:link_inertial mass="${link5_mass}" ixx="0.002084999166" iyy="0.002084999166" izz="0.00225"/>
      <visual>
        <geometry>
          <mesh filename="file://$(find ur_custom_description)/meshes/ur3e/visual/wrist1.dae"/>
        </geometry>
        <origin xyz="0 0 -0.104" rpy="1.5707963267948966 0 0"/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find ur_custom_description)/meshes/ur3e/collision/wrist1.stl"/>
        </geometry>
        <origin xyz="0 0 -0.104" rpy="1.5707963267948966 0 0"/>
      </collision>
    </link>

    <link name="${prefix}link6">
      <xacro:link_inertial mass="${link6_mass}" ixx="0.002084999166" iyy="0.002084999166" izz="0.00225"/>
      <visual>
        <geometry>
          <mesh filename="file://$(find ur_custom_description)/meshes/ur3e/visual/wrist2.dae"/>
        </geometry>
        <origin xyz="0 0.00 -0.08535" rpy="0 0 0"/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find ur_custom_description)/meshes/ur3e/collision/wrist2.stl"/>
        </geometry>
        <origin xyz="0 0.00 -0.08535" rpy="0 0 0"/>
      </collision>
    </link>

    <link name="${prefix}${flange_link}">
      <xacro:link_inertial mass="${flange_mass}" ixx="0.00013626661215999998" iyy="0.00013626661215999998" izz="0.0001792"/>
      <visual>
        <geometry>
          <mesh filename="file://$(find ur_custom_description)/meshes/ur3e/visual/wrist3.dae"/>
        </geometry>
        <origin xyz="0.0 0 -0.0921" rpy="1.5707963267948966 0 0"/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="file://$(find ur_custom_description)/meshes/ur3e/collision/wrist3.stl"/>
        </geometry>
        <origin xyz="0.0 0 -0.0921" rpy="1.5707963267948966 0 0"/>
      </collision>
    </link>

    <joint name="${prefix}${base_link}_to_${prefix}link1" type="fixed">
      <parent link="${prefix}${base_link}"/>
      <child link="${prefix}link1"/>
      <xacro:insert_block name="origin"/>
      <origin xyz="0 0 0" rpy="0 0 3.141592653589793"/>
    </joint>

    <joint name="${prefix}link1_to_${prefix}link2" type="revolute">
      <axis xyz="0 0 1"/>
      <limit effort="${joint_effort}" lower="-6.283185307179586" upper="6.283185307179586" velocity="${joint_velocity}"/>
      <parent link="${prefix}link1"/>
      <child link="${prefix}link2"/>
      <origin xyz="0 0 0.15185" rpy="0 0 0"/>
      <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
    </joint>

    <joint name="${prefix}link2_to_${prefix}link3" type="revolute">
      <axis xyz="0 0 1"/>
      <limit effort="${joint_effort}" lower="-6.283185307179586" upper="6.283185307179586" velocity="${joint_velocity}"/>
      <parent link="${prefix}link3"/>
      <child link="${prefix}link4"/>
      <origin xyz="0 0 0" rpy="1.570796327 0 0/>
      <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
    </joint>

    <joint name="${prefix}link3_to_${prefix}link4" type="revolute">
      <axis xyz="0 0 1"/>
      <limit effort="28.0" lower="-3.141592653589793" upper="3.141592653589793" velocity="${joint_velocity}"/>
      <parent link="${prefix}link3"/>
      <child link="${prefix}link4"/>
      <origin xyz="-0.24355 0 0" rpy="0 0 0/>
      <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
    </joint>

    <joint name="${prefix}link4_to_${prefix}link5" type="revolute">
      <axis xyz="0 0 1"/>
      <limit effort="12.0" lower="-6.283185307179586" upper="6.283185307179586" velocity="6.283185307179586"/>
      <parent link="${prefix}link4"/>
      <child link="${prefix}link5"/>
      <origin xyz="-0.2132 0 0.13105" rpy="0 0 0"/>
      <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
    </joint>

    <joint name="${prefix}link5_to_${prefix}link6" type="revolute">
      <axis xyz="0 0 1"/>
      <limit effort="12.0" lower="-6.283185307179586" upper="6.283185307179586" velocity="6.283185307179586"/>
      <parent link="${prefix}link5"/>
      <child link="${prefix}link6"/>
      <origin xyz="0 -0.08535 -1.750557762378351e-11" rpy="1.570796327 0 0"/>
      <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
    </joint>

    <joint name="${prefix}link6_to_${prefix}${flange_link}" type="revolute">
      <axis xyz="0 0 1"/>
      <limit effort="12.0" lower="-6.283185307179586" upper="6.283185307179586" velocity="6.283185307179586"/>
      <parent link="${prefix}link6"/>
      <child link="${prefix}${flange_link}"/>
      <origin xyz="0 0.0921 -1.8890025766262e-11" rpy="1.570796326589793 3.141592653589793 3.141592653589793"/>
      <dynamics damping="${joint_damping}" friction="${joint_friction}"/>
    </joint>

    <xacro:material_visual ref_link="${prefix}link1" ambient="0.5 0.5 0.5 1" diffuse="0.5 0.5 0.5 1" specular="0.5 0.5 0.5 1"/>
    <xacro:material_visual ref_link="${prefix}link2" ambient="1 1 1 1" diffuse="1 1 1 1" specular="1 1 1 1"/>
    <xacro:material_visual ref_link="${prefix}link3" ambient="1 1 1 1" diffuse="1 1 1 1" specular="1 1 1 1"/>
    <xacro:material_visual ref_link="${prefix}link4" ambient="1 1 1 1" diffuse="1 1 1 1" specular="1 1 1 1"/>
    <xacro:material_visual ref_link="${prefix}link5" ambient="1 1 1 1" diffuse="1 1 1 1" specular="1 1 1 1"/>
    <xacro:material_visual ref_link="${prefix}link6" ambient="1 1 1 1" diffuse="1 1 1 1" specular="1 1 1 1"/>
    <xacro:material_visual ref_link="${prefix}${flange_link}" ambient="0.5 0.5 0.5 1" diffuse="0.5 0.5 0.5 1" specular="0.5 0.5 0.5 1"/>

  </xacro:macro>
</robot>